{
  "name": "hive-greeter",
  "version": "1.0.0",
  "description": "Discord bot written in Typescript and discord.js hosted on Heroku",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config build/index.js",
    "build": "npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/index.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ACM-VIT/acm-internal-discord-chatbot"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Gokul J Kurup (madrigal1)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ACM-VIT/acm-internal-discord-chatbot/issues"
  },
  "homepage": "https://github.com/ACM-VIT/acm-internal-discord-chatbot#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.20",
    "@types/winston": "^2.4.4",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
